    <connection>MySQL_CONNECTION</connection>
    <sql>SELECT
  simat.number_document as Cedula,
	CONCAT(simat.last_name,' ',second_surname,' ',simat.name,' ',simat.second_name) as Estudiante,
	grades.name as Grado,
	grupo.name as Grupo,
	sede.name as Sede,
	ie.name as Institucion,
	towns.name as Municipio,
	dtops.name as Departamento,
	simat.gender as Genero,
	simat.age as Edad,
	simat.ethnicity as Etnia,
	simat.disability as Discapacidad,
	pn.calc_indicator as PromedioNotas,
	pg.calc_indicator as ProgresoGlobal,
	lg.calc_indicator as UltimoLogeo,
	students.created_date as fecha
FROM
  simat simat
LEFT JOIN students students
  ON simat.id = students.simat_id
LEFT JOIN headquarters_has_grade_has_group_has_user hggu
  ON students.id = hggu.student_id
LEFT JOIN headquarters_has_grade_has_group hgg
  ON hggu.headquarters_has_grade_has_group_id = hgg.id
LEFT JOIN grades grades
  ON hgg.grade_id = grades.id
LEFT JOIN `groups` grupo
  ON grupo.id = hgg.group_id
LEFT JOIN headquarters sede
  ON sede.id = hgg.headquarters_id
LEFT JOIN institutions ie
  ON ie.id = sede.institution_id
LEFT JOIN towns towns
  ON towns.id = sede.town_id
LEFT JOIN dtops dtops
  ON dtops.id = towns.dtop_id
LEFT JOIN student_indicators pn
  ON pn.student_id = students.id AND pn.indicator_id = 5
LEFT JOIN student_indicators pg
  ON pg.student_id = students.id AND pg.indicator_id = 9
LEFT JOIN student_indicators lg
  ON lg.student_id = students.id AND lg.indicator_id = 2</sql>
